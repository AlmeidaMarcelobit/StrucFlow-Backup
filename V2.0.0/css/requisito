    <script>
        function filtrarEquipamentos() {
            const search = document.getElementById('search').value.toLowerCase();
            const statusFilter = document.getElementById('status-filter').value;
            const equipamentos = document.querySelectorAll('.equipamento-card');

            equipamentos.forEach(equipamento => {
                const tipo = equipamento.querySelector('h3').textContent.toLowerCase();
                const status = equipamento.querySelector('.status').classList.contains('disponivel') ? 'disponivel' : 'indisponivel';

                const matchSearch = tipo.includes(search);
                const matchStatus = statusFilter === '' || status === statusFilter;

                if (matchSearch && matchStatus) {
                    equipamento.style.display = 'block';
                } else {
                    equipamento.style.display = 'none';
                }
            });
        }
    </script>
